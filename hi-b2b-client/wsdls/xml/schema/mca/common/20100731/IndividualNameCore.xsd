<xsd:schema 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://ns.electronichealth.net.au/hi/xsd/common/IndividualNameCore/3.0" 
	xmlns:cin="http://ns.electronichealth.net.au/hi/xsd/common/IndividualNameCore/3.0" 
	xmlns:cdt="http://ns.electronichealth.net.au/hi/xsd/common/CommonCoreDatatypes/3.0" 
	elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xsd:annotation>
		<xsd:documentation>
==============================  IMPORTS  =======================================
		</xsd:documentation>
	</xsd:annotation>    
	
	<xsd:import 
        namespace="http://ns.electronichealth.net.au/hi/xsd/common/CommonCoreDatatypes/3.0" 
        schemaLocation="CommonCoreDatatypes.xsd"/>	

	<xsd:annotation>
		<xsd:documentation>
==============================  ELEMENTS =======================================
		</xsd:documentation>
	</xsd:annotation>

    <xsd:element name="nameTitle" type="cdt:NameTitleType"/>
	<xsd:element name="familyName" type="cdt:FamilyNameType"/>
    <xsd:element name="givenName" type="cdt:GivenNameType"/>
	<xsd:element name="nameSuffix" type="cdt:NameSuffixType"/>
	<xsd:element name="usage" type="cdt:IndividualNameUsageType"/>

	<xsd:annotation>
		<xsd:documentation>
==============================  COMPLEX TYPES ==================================
		</xsd:documentation>
	</xsd:annotation>

</xsd:schema>